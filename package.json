{
  "name": "promptsu-ai",
  "private": true,
  "scripts": {
    "check": "oxlint",
    "dev": "turbo dev -F=server -F=web",
    "start": "turbo start -F=server -F=web",
    "build": "turbo build ",
    "build:web": "turbo build -F web build",
    "check-types": "turbo check-types",
    "lint": "ultracite format",
    "format": "bunx @biomejs/biome check ./apps --write",
    "ruler:apply": "bunx @intellectronica/ruler@latest apply --local-only",
    "dev:doc": "turbo -F native doc",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "db:push": "turbo -F server db:push",
    "db:studio": "turbo -F server db:studio",
    "db:generate": "turbo -F server db:generate",
    "db:migrate": "turbo -F server db:migrate"
  },

  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "oxlint": "^1.12.0",
    "tailwindcss": "3",
    "ultracite": "5.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "npx ultracite format"
    ]
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@ai-sdk/google": "^2.0.11",
    "ai": "^5.0.25"
  }
}
