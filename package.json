{
  "name": "promptsu-ai",
  "private": true,
  "scripts": {
    "check": "oxlint",
    "dev": "turbo dev -F=server ",
    "dev:express": "turbo dev -F=express ",
    "start": "turbo start -F=server ",
    "build": "turbo build -F=server ",
    "check-types": "turbo check-types",
    "lint": "ultracite format",
    "knip": "knip",
    "format": "bunx @biomejs/biome check ./apps --write",
    "ruler:apply": "bunx @intellectronica/ruler@latest apply --local-only",
    "dev:doc": "turbo -F native doc",
    "dev:server": "turbo -F server dev",
    "db:push": "turbo -F server db:push",
    "db:studio": "turbo -F server db:studio",
    "db:generate": "turbo -F server db:generate",
    "db:migrate": "turbo -F server db:migrate"
  },
  "resolutions": {
    "lucide-react": "^0.542.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "oxlint": "^1.14.0",
    "tailwindcss": "3",
    "turbo": "^2.5.6",
    "ultracite": "5.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "pnpm  dlx  ultracite format"
    ]
  },
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@ai-sdk/google": "^2.0.11",
    "@scalar/hono-api-reference": "^0.9.18",
    "ai": "^5.0.25",
    "better-auth": "^1.3.9",
    "biome": "^0.3.3",
    "knip": "^5.63.1",
    "ngrok": "^5.0.0-beta.2"
  }
}
